class Solution:
    def minSubArrayLen(self, target: int, nums: List[int]) -> int: 
        
        start = 0
        result = len(nums) + 1
        
        for end in range(len(nums)):
            target -= nums[end]
            
            while target <= 0:
                print(end, start)
                result = min(result, end - start + 1)
                target += nums[start]
                start += 1
        print()
        return result % (len(nums) + 1)
